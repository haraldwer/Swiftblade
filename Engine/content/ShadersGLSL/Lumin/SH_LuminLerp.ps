#include "Shaders/Uniforms/SH_FrameUniforms.si"

uniform sampler2D TexSH;
uniform sampler2D TexFrameSH;

in vec2 TexCoord;

layout(location = 0) out vec4 OutSH;

void main()
{
    // Could probably do with just some alpha blending... 
    vec4 frameSH = texture(TexFrameSH, TexCoord);
    vec4 lerpSH = texture(TexSH, TexCoord);
    OutSH = mix(lerpSH, frameSH, 0.1f);
}