#version 330

#include "Shaders/Uniforms/SH_FrameUniforms.si"
#include "Shaders/Uniforms/SH_TexUniforms.si"

in vec2 TexCoord;
out vec4 Output;

void main()
{
    vec4 col = texture(TexFrame, InCoord);
    if (col.a < 0.01)
        discard;
    Output.rgb = col.rgb;
    Output.a = SampleWorldPos(TexCoord).a; 
}