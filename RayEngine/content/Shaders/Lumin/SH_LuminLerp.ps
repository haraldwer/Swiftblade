#include "Shaders/Uniforms/SH_FrameUniforms.si"

uniform sampler2D TexFrameIrradiance;
uniform sampler2D TexFramePrefilter;

in vec2 TexCoord;
out vec4 OutIrradiance;
out vec4 OutPrefilter;

void main()
{
    OutIrradiance = texture(TexFrameIrradiance, TexCoord);
    OutPrefilter = texture(TexFramePrefilter, TexCoord);
    OutIrradiance.a = 0.1;
    OutPrefilter.a = 0.1;
}